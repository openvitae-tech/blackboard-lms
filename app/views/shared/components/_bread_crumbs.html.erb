<div class="my-4">
  <ol class="flex items-center space-x-2 text-secondary text-xs">
    <% links.each do |title, link| %>
    <li class="flex space-x-1">
      <%#= link_to title, link %>
      <span><%= title %></span>
      <span class="icon icon-simple-arrow-right h-4 w-4"></span>
    </li>
    <% end %>
  </ol>
</div>
